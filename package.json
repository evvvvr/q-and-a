{
  "name": "q-and-a-app",
  "version": "0.0.1",
  "description": "Simple Q&A App Like Stackoverflow.com",
  "engines": {
    "node": "4.2.4"
  },
  "scripts": {
    "build:server": "babel src --ignore src/client --out-dir build && cp -r src/db build",
    "watch:server": "babel src --watch --ignore src/client --out-dir build",
    "build:client": "webpack --config ./webpack.config.js --progress",
    "watch:client": "webpack --watch --config ./webpack.config.js --progress",
    "build": "npm-run-all build:server build:client",
    "watch": "npm-run-all --parallel watch:server watch:client",
    "postinstall": "npm run-script build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "body-parser": "^1.13.3",
    "classnames": "^2.2.1",
    "css-loader": "^0.16.0",
    "es6-error": "^2.1.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "jsonschema": "^1.0.2",
    "keymirror": "^0.1.1",
    "moment": "^2.10.6",
    "normalize.css": "^3.0.3",
    "pg-promise": "^3.3.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-immutable-proptypes": "^1.5.1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.3",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-saga": "^0.9.3",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "npm-run-all": "^1.5.1"
  }
}
